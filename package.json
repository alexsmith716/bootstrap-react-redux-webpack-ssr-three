{
  "private": true,
  "name": "bootstrap-react-redux-webpack-ssr-three",
  "version": "0.0.1",
  "main": "bin/server.js",
  "description": "App is a continuation of repo bootstrap-react-redux-webpack-ssr-two.",
  "author": "dev",
  "license": "MIT",
  "scripts": {
    "run:dev": "npm-run-all dev:prepare development",
    "dev:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development": "NODE_ENV=development npm-run-all --parallel dev:client:build dev:server:build start:dev",
    "dev:server": "NODE_ENV=development WEBPACK_DLLS=1 nodemon ./bin/server.js --watch ./server --watch ./build/server --no-update-notifier",
    "dev:api": "NODE_ENV=development nodemon ./bin/api.js --watch ./api --no-update-notifier",
    "dev:client:build": "NODE_PATH=client NODE_ENV=development WEBPACK_DLLS=1 node ./webpack/webpack.dev.server.js",
    "dev:server:build": "NODE_ENV=development webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --display-error-details",
    "start:dev": "concurrently \"yarn dev:server\" \"yarn dev:api\"",
    "start:prod": "concurrently \"yarn prod:server\" \"yarn prod:api\"",
    "prod:server": "NODE_ENV=production node ./bin/server.js",
    "prod:api": "NODE_ENV=production node ./bin/api.js",
    "prod:server:build": "NODE_ENV=production webpack --mode production --display-error-details --config ./webpack/webpack.config.server.production.babel.js",
    "dev:dlls": "webpack --display-error-details --config webpack/dllplugin.vendor.config.js",
    "prod:build": "NODE_ENV=production webpack --mode production --display-error-details --config ./webpack/webpack.config.client.production.babel.js",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(bin)/**/*.js'",
    "lint": "eslint -c .eslintrc bin && flow",
    "test": "NODE_PATH=client jest client"
  },
